<%- include('partials/_header')  %> 
      
      <%- include('partials/_navigation')  %> 
      <!-- header section end -->
      <!-- blog section start -->
      <div class="blog_section layout_padding">
         <div class="container">
            <div class="row">
               <div class="col-md-12">
                  <% if (category) { %> 
                  <h1 class="blog_taital"><%= category.name  %></h1>
                  <% } %> 
               </div>
            </div>
         </div>
         <div class="container mb-2">
            <div class="blog_section_2 layout_padding">
               <div class="row">
                  <% for (let i = 0; i<furnitures.length;i++) { %>
                     <div class="col-lg-4 col-md-4 col-sm-12">
                        <div class="blog_box">
                           <div class="blog_img"><img src="/<%= furnitures[i].image %>"></div>
                           <p class="chair_text"><a href="/furniture/single/<%= furnitures[i].slug  %>"><%= furnitures[i].name.substring(0,25) + '...'  %></a> <br> Price: <%= furnitures[i].price  %>$</p>
                        </div>
                     </div>
                  <% } %> 
               </div>
            </div>
            <div class="readmore_bt"><a href="#">See More</a></div>
         </div>
      </div>
      <!-- blog section end -->
      <!-- footer section start -->
 <%- include('partials/_footer')  %> 