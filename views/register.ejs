        <div class="col-sm-12">
           <h1 class="contact_taital contact_heading">Register</h1>
        </div>
     </div>
     <div class="contact_section_2">
        <div class="row">
           <div class="col-md-12">
              <div class="mail_section map_form_container">
                 <form action="">
                    <input type="text" class="mail_text" placeholder="Name" name="name">
                    <input type="text" class="mail_text" placeholder="Email" name="email">
                    <select class="mail_text" name="role">
                      <option>member</option>
                      <option>trainer</option>
                  </select>
                    <input type="text" class="mail_text mb-4" placeholder="Password" name="Password">
                    <div class="btn_main">
                      <button type="submit" value="LOGIN" id="submit" class="btn btn-warning btn-radius btn-brd btn-block text-white">REGISTER</button>
                    </div>
                 </form>
              </div>
           </div>
        </div>
     </div>
  </div>
</div>
